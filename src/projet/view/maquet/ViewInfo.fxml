<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>


<VBox prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="projet.view.maquet.ControllerInfo">
   <children>
      <Label fx:id="labelTitre" alignment="CENTER" styleClass="titre-vue" text="Titre">
         <VBox.margin>
            <Insets left="250.0" top="50.0" />
         </VBox.margin>
      </Label>
      <Label fx:id="labelMessage" alignment="CENTER" styleClass="message-accueil" text="Message">
         <VBox.margin>
            <Insets left="240.0" top="50.0" />
         </VBox.margin>
      </Label>
      <Button alignment="TOP_CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#doAccueil" text="Page Principale">
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
         <VBox.margin>
            <Insets left="260.0" top="50.0" />
         </VBox.margin>
      </Button>
   </children>
</VBox>
